def add_binary(s1, s2):
    # Convert binary strings to decimal integers
    n1 = int(s1, 2)
    n2 = int(s2, 2)
    
    # Add the decimal numbers
    sum = n1 + n2
    
    # Convert the sum back to binary string
    result = bin(sum)[2:]
    
    return result
s1 = "1101"
s2 = "100101"
print(add_binary(s1, s2))  # Output: 110010

s1 = "1111001001111101"
s2 = "10000"
print(add_binary(s1, s2))  # Output: 1111001010001101

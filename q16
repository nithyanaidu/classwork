#include <stdio.h>

void swap(int *x, int *y) {
    int temp = *x;
    *x = *y;
    *y = temp;
}

int main() {
    int num1 = 10, num2 = 20;
    
    printf("Before swapping, num1 = %d and num2 = %d\n", num1, num2);
    
    swap(&num1, &num2);
    
    printf("After swapping, num1 = %d and num2 = %d\n", num1, num2);
    
    return 0;
}
ouput
Before swapping, num1 = 10 and num2 = 20
After swapping, num1 = 20 and num2 = 10
